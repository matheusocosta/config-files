"*":
  core:
    autoHideMenuBar: true
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "node_modules"
    ]
    projectHome: "E:/Projetos"
    themes: [
      "one-dark-ui"
      "twilight-seriously"
    ]
  editor:
    fontFamily: "Code New Roman, Consolas"
    invisibles:
      cr: " "
      eol: " "
      tab: " "
    lineHeight: 1.6
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
  "file-icons":
    coloured: false
  linter: {}
  "linter-js-standard": {}
  "markdown-preview":
    useGitHubStyle: true
  minimap:
    absoluteMode: true
  "open-in-browsers":
    IE: false
    Opera: false
    Safari: false
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  pigments:
    markerType: "dot"
  "tree-view": {}
  welcome:
    showOnStartup: false
